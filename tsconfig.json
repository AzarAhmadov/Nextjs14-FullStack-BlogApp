{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/lib/utils.js", "src/lib/models.ts", "src/app/loading.tsx", "src/components/RegisterForm/RegisterForm.tsx", "src/lib/auth.config.js", "src/middleware.js", "src/lib/auth.ts", "src/components/LoginForm/LoginForm.jsx", "src/components/AdminPostForm/AdminPostForm.jsx"],
  "exclude": ["node_modules"]
}
